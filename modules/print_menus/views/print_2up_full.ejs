    <% let header_colors = ['#24adc5', '#ef5b48', '#d0cc44', '#ea1c5f', '#706faa', '#24adc5', '#ef5b48', '#d0cc44', 
    '#ea1c5f', '#706faa', '#24adc5', '#ef5b48', '#d0cc44', '#ea1c5f', '#706faa', '#24adc5', '#ef5b48', '#d0cc44', 
    '#ea1c5f', '#706faa'];
    %>
    <div class="beer-grid">
        <div class="ui two column grid">
            <div class="row">
                <div class="beer column background left">
                    <div class="menu-header left">
                        <img class="logo" src="/img/menu-boards/white_square_logo.png">
                        <img class="all-the-beer" src="/img/menu-boards/TAPS-pink.png">
                    </div>
                    <div class="beer-list left">
                        <% let sections = taps_menu.menu.sections; %> 
                        <% for (var h = 0; h < sections.length; h++) { %>
                            <div class="row">
                                <div class="header-text" style="color:<%=header_colors[h]%>">
                                    <% let section_name = sections[h].name.toUpperCase(); %>
                                    <%=section_name%>
                                    
                                </div>
                               <% let items = sections[h].items; %>
                               <% for (var i = 0; i < items.length; i++) { %>
                                    <div class="row">
                                        <div class="top">
                                            <span class="beer-name line1"><%=items[i].name %></span>
                                            <span class="beer-brewery line1">
                                                <% 
                                                    let brewery = items[i].brewery;
                                                        brewery = brewery.replace('Company', '');
                                                        brewery = brewery.replace('Co.', '');
                                                        brewery = brewery.replace('Cooperative', '')
                                                        brewery = brewery.replace('Brewing', '')
                                                %>
                                                (<%= brewery %>)
                                            </span>
                                            <span class="beer-style">
                                                <% 
                                                    let style = items[i].style;
                                                        style = style.substring(0, style.indexOf('-'));
                                                        style = style.toUpperCase();
                                                %>
                                                <% if (style) { %>
                                                    <%= style %>
                                                <% } else { %>
                                                    <%= items[i].style %>
                                                <% } %>
                                           | </span>
                                            <span class="beer-abv_price">
                                                <% let price = items[i].containers[0].price%>
                                                  <% let size = items[i].containers[0].container_size.name%>
                                                    
                                                
                                                
                                                <%= items[i].abv%>% <%=price%> <%=size%>
                                            </span>
                                            
                                        </div>
                                    </div>
                               <% } %>
                            </div>
                        <% } %>
                    </div>
                </div>
                <div class="beer column background right">
                     <div class="menu-header right">
                        <img class="logo" height="50px" src="/img/menu-boards/white_square_logo.png">
